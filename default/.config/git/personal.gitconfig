[user]
  name = Miguel Luis
  email = redacted
  signinkey = redacted 
[commit]
  gpgsign = true
[init]
  defaultBranch = main
[pull]
  rebase = false
[core]
  editor = nvim
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
  excludesfile = ~/.config/git/.gitignore_global
  autocrlf = false
[filer "lfs"]
  required = true
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
[web]
  browser = chrome
